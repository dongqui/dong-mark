{
  "manifest_version": 3,
  "name": "Dong mark",
  "description": "This is Dong mark",
  "version": "1.0.0",
  "permissions": [ 
    "activeTab",
    "tabs"
  ],  
  "action" : {
      "default_popup": "popup.html",
      "default_title": "Dong mark"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
    }
]
}